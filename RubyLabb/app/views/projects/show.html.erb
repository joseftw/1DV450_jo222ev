<h1>Project details. </h1>

<h2 id="singleProjectTitle"><%= @project.name %></h2>
<p id="singleProjectDescription"><%= @project.description %></p>
<label for="singleProjectMemeberList">Project members</label>
<ul ="singleProjectMemberList">
  <% @project.users.each do |user| %>
    <li><%= link_to user.first_name, user_path(user) %></li>
  <% end %>
</ul>